{
    "manifest_version": 3,
    "name": "AO3 Reading Time",
    "version": "1.0",
    "description": "Small Chrome Extension that adds reading time into AO3 works!",
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      },
      
    "content_scripts": [
        {
            "js": ["scripts/content.js"],
            "matches": [
                "https://archiveofourown.org/works/*"
            ]
        }
    ]
}